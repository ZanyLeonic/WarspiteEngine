{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug x86 build-deps script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/build-deps-x86.ps1",
            "args": [
                "C:\\vcpkg\\",
                "--CI"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug x64 build-deps script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/build-deps.ps1",
            "args": [
                "Release",
                "test-release"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Test Export Map",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/sdk/tools/ExportMap.py",
            "args": [
                "-map",
                "${workspaceFolder}/sdk/maps/map02.json",
                "-workDir",
                "${workspaceFolder}/build/Debug"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Test Run Map (with Engine)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/sdk/tools/RunEngine.py",
            "args": [
                "-map",
                "${workspaceFolder}/sdk/maps/map02.json",
                "-engine",
                "${workspaceFolder}/build/Release/ws.exe"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Random Map Generator",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/sdk/tools/MapGenerator/main.py",
            "args": [
                "${workspaceFolder}/sdk/maps/untitled.json"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Download Python Embed",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Engine/src/DownloadPython.py",
            "args": [
                "E:\\Projects\\WarspiteGame\\build",
                "3.8.7",
                "amd64",
                "Debug"
            ],
            "console": "integratedTerminal"
        }
    ]
}